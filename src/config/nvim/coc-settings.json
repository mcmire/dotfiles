{
  "[astro][css][ghmarkdown][javascript][javascriptreact][json][markdown][ruby][rust][scss][typescript][typesscriptreact][vue]": {
    "coc.preferences.formatOnSave": true,
  },
  "coc.preferences.formatOnType": false,
  "coc.preferences.jumpCommand": "tabe",
  "diagnostic.checkCurrentLine": true,
  "diagnostic.enableMessage": "jump",
  "diagnostic.floatConfig": {
    "maxWidth": 200,
    "maxHeight": 1500
  },
  "diagnostic.messageDelay": 1000,
  "diagnostic.refreshOnInsertMode": true,
  "eslint.autoFixOnSave": false,
  "eslint.experimental.useFlatConfig": false,
  "eslint.fixOnSaveTimeout": 10000,
  "eslint.format.enable": true,
  "eslint.probe": [
    "astro",
    "css",
    "javascript",
    "javascriptreact",
    "json",
    "markdown",
    "ruby",
    "rust",
    "scss",
    "typescript",
    "typescriptreact",
    "vue"
  ],
  "eslint.trace.server": "verbose",
  "eslint.validate": [
    "astro",
    "css",
    "ghmarkdown",
    "javascript",
    "javascriptreact",
    "json",
    "ruby",
    "rust",
    "scss",
    "typescript",
    "typescriptreact",
    "vue"
  ],
  "hover.floatConfig": {
    "maxWidth": 200,
    "maxHeight": 1500
  },
  "javascript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBraces": true,
  "javascript.suggest.enabled": false,
  "javascript.suggest.completeFunctionCalls": false,
  "javascript.suggestionActions.enabled": false,
  "javascript.validate.enable": true,
  "json.format.enable": false,
  "prettier.documentSelectors": ["**/*.astro"],
  "prettier.requireConfig": true,
  "signature.enable": false,
  "stylelintplus.autoFixOnSave": false,
  "suggest.autoTrigger": "none",
  "suggest.enableFloat": false,
  "tsserver.log": "verbose",
  "tsserver.trace.server": "verbose",
  "typescript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBraces": true,
  "typescript.preferences.importModuleSpecifier": "project-relative",
  "typescript.suggest.enabled": false,
  "typescript.suggest.completeFunctionCalls": false,
  "typescript.suggestionActions.enabled": false
}
